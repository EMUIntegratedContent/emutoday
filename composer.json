{
    "name": "emich/emutoday",
    "description": "Eastern Michigan University news, magazine articles, events and announcements.",
    "type": "project",
    "license": "proprietary",
    "version": "1.2.0",
    "homepage": "https://today.emich.edu",
    "require": {
        "php": ">=7.3",
        "laravel/framework": "^5.6.40",
        "laravel/passport": "~5.0",
        "laravelcollective/html": "5.5.*",
        "laracasts/presenter": "^0.2.4",
        "laracasts/utilities": "~2.1",
        "laracasts/flash": "^2.0",
        "intervention/image": "^2.7",
        "intervention/imagecache": "^2.4",
        "league/fractal": "^0.14.0",
        "sofa/eloquence": "~5.6",
        "barryvdh/laravel-debugbar": "^3.4",
        "subfission/cas": "^2.1.1",
        "google/apiclient": "dev-main",
        "barryvdh/laravel-cors": "^0.11.4",
        "doctrine/dbal": "~2.13",
        "snowfire/beautymail": "^1.1.5",
        "guzzlehttp/guzzle": "^6.5",
        "laravel/tinker": "^1.0",
        "laravelium/feed": "2.12.*",
        "laravel/homestead": "8.0.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^7.0",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Emutoday\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
